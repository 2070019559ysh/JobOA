@{
    SelectList selectList = ViewData["list"] as SelectList;
}
<select name="@ViewData.TemplateInfo.HtmlFieldPrefix" multiple data-am-selected="{btnSize: 'sm', btnStyle: 'secondary',maxHeight: 150,searchBox: 1}">
    @if (selectList != null)
    {
        foreach (var item in selectList)
        {
            System.Collections.IEnumerator e = selectList.SelectedValues.GetEnumerator();
            do
            {
                e.MoveNext();
                try
                {
                    object obj = e.Current;
                }
                catch (Exception)
                {
                    break;
                }
                if (e.Current.ToString().Equals(item.Value))
                {
                    <option selected="selected" value="@item.Value">@item.Text</option>
                }
                else
                {
                    <option value="@item.Value">@item.Text</option>
                }
            } while (true);
        }

    }
    else
    {
        <option value="">无</option>
    }
</select>